<div class="gwsub sb04">
<%= render 'gwsub/admin/sb04/sb04_piece_header' %>
<div class="pieceBanner">
<%= render 'gwsub/admin/sb04/sb04_subfunction_select_menu' %>
<%= render 'gwsub/admin/sb04/sb04_level3menu_header' %>
<%= render 'gwsub/admin/sb04/sb04assignedjobs/display_name' %>
</div>
<div class="pieceBody">

  <table class="show">
    <tbody>
      <tr><th>年度</th><td><%= @item.fyear_markjp -%></td></tr>
      <tr><th>所属コード</th><td><%= @item.section_code -%></td></tr>
      <tr><th>所属名</th><td><%= @item.section_name -%></td></tr>
      <tr><th>担当コード</th><td><%= @item.code -%></td></tr>
      <tr><th>担当名</th><td><%= @item.name -%></td></tr>
      <tr><th>担当電話</th><td><%= @item.tel -%></td></tr>
      <tr><th>担当住所</th><td><%= Gw.br(@item.address) -%></td></tr>
      <tr><th>備考</th><td><%= @item.remarks -%></td></tr>
    </tbody>
  </table>
  <div class="btReturn">
  <%= link_to '一覧に戻る',"#{@csv_base_url}/index_csv?#{@qs}" %>
  </div>
</div>
</div>
