<div class="bbsDocList workflow">
<%= render 'piece_header' %>

<div class="pieceBody">
<div class="formTitle edit">編集</div>

<%= form_for :item, url: {action: :update}, html: {method: :put, multipart: true, id: 'itemForm'} do |f| %>
  <%= f.error_messages %>
  <%= render 'form', f: f %>
  <div class="submitters">
    <%= submit_for_update f, js_submitter: "my_submit()", caption: '変更する' %>
  </div>
<% end %>

</div><!--pieceBody-->
</div><!--bbsDocList workflow-->
