<div class="bbsDocList workflow">
<%= render 'piece_header' %>

<div class="pieceBody">
<br />

<%= form_for :item, url: {action: :update}, html: {:method => :put} do |f| %>
  <%= f.error_messages %>
  <%= render 'form', f: f %>
  <div class="submitters"><%= f.submit '保存' %></div>
<% end %>

<!--戻るボタン-->
<div class="btReturn">
  <%= link_to '設定へ戻る', "/gwworkflow/settings" %>
</div>

</div><!--pieceBody-->
</div><!--bbsDocList workflow-->
