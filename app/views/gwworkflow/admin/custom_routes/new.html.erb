<div class="bbsDocList workflow">
<%= render 'piece_header' %>

<div class="pieceBody">
<div class="formTitle new">新規作成</div>

<%= form_for :item, url: {action: :create}, html: {method: :post, multipart: true, id: 'itemForm'} do |f| %>
  <%= f.error_messages %>
  <%= render 'form', f: f %>
  <div class="submitters">
    <%= submit_for_update f, js_submitter: "my_submit()", caption: '登録する' %>
  </div>
<% end %>

</div><!--pieceBody-->
</div><!--bbsDocList workflow-->
