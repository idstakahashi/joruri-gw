<div class="bbsDocList circular setting">
<%= render 'piece_header' %>

<!--カラム用タグ-->
<div class="pieceBanner">
</div>
<div class="pieceBody">

<%= form_for :item, :url => @item.update_path ,:html => {method: :put, multipart: true, id: 'itemForm'} do |f| %>
  <%= f.error_messages %>
  <%= render 'form', f: f %>
  <div class="submitters">
    <%= submit_for_update f, js_submitter: "my_submit()", caption: '登録する' -%>
  </div>
<% end %>

</div><!--pieceBody-->
</div><!--piece gwbbs-->
