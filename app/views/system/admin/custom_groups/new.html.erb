<div class="piece AdminMessage">
<div class="pieceHeader">
  <h2 class="pieceTitle">カスタムグループ</h2>
</div><!--pieceHeader-->
<div class="pieceBody">

<%= form_for @item, :as => :item, :url => {action: :create}, :html => {method: :post, id: 'itemForm'} do |f| %>
  <%= f.error_messages %>
  <%= render 'form', f: f %>
  <div class="submitters"><%= f.submit '保存' %></div>
<% end %>

</div><!--pieceBody-->
</div><!--piece-->
