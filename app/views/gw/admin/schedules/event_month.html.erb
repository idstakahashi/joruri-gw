<%
@hide_user_col = true
%>
<div class="piece schedulePiece monthview">
<div class="pieceHeader">
  <%= render '/gw/admin/schedules/piece_header', d: @st_date, mode: 'month' -%>
</div>
<div class="pieceBody">
<div class="pieceBodyHeader">
  <%= render '/gw/admin/schedules/schedule_header', d: @st_date, mode: 'month' -%>
</div>
<div class="pieceBodyBody">
<% if @is_ev_admin || @is_csv_put_user %>
  <div class="functionBox">
    <%= link_to "CSV出力", "/gw/schedules/event_month.csv?s_date=#{@st_date.strftime('%Y%m%d')}" %>
  </div>
<% end %>

<%= render '/gw/admin/schedules/event_month_part' %>

</div>
<div class="pieceBodyFooter">
  <%= render '/gw/admin/schedules/schedule_footer', d: @st_date, mode: 'month' %>
</div>
</div>
</div>
