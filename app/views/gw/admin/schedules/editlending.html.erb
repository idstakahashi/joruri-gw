<div class="piece schedulePiece form">
<div class="pieceHeader">
<%= render '/gw/admin/schedules/piece_header' %>
</div>
<div class="pieceBody">
<div class="pieceBodyHeader">
  <%= render '/gw/admin/schedules/schedule_bt_box', d: Date.today, mode: 'form' %>
</div>

<div class="pieceBodyBody scheduleform">
<%= form_for :item, url: "/gw/schedules/#{@item.id}/editlending#{@edit_params}", html: {method: :put, multipart: true, id: 'itemForm'} do |f| %>
  <%= f.error_messages %>
  <%= render 'form_lending', f: f %>
  <%= submit_for_update f, js_submitter: 'return my_submit();', class: :mockblock %>
<% end %>
</div>
</div>
</div>
