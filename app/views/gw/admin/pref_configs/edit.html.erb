<div class="piece AdminSetting">
<%= render 'header_str' %>

<div class="pieceBody">

<%= form_for :item, :url => {:action => :update}, :html => {:method => :put} do |f| %>
  <%= f.error_messages %>
  <%= render 'form', f: f %>
  <%= submit_for f, {:caption => '保存'} %>
<% end %>

<div class="btReturn">
  <%= link_to '戻る', @return_uri %>
</div>

</div><!--pieceBody-->
</div><!--piece admin_messages-->
