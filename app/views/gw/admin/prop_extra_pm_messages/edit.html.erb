<div class="piece AdminSetting">
<div class="pieceHeader">
<%= render 'piece_header' -%>
</div>
<div class="pieceBody">
<div class="pieceBodyHeader">
<%= render 'header' %>
</div>
<div class="pieceBodyBody">

<%= form_for :item, :url => {:action => :update}, :html => {:method => :put, :multipart => true, :id => 'itemForm'} do |f| -%>
  <%= f.error_messages -%>
  <%= render 'form', f: f -%>
  <div class="preserve">
    <%= f.submit '保存' %>
  </div>
<% end -%>

</div>
</div>
</div>
