<table class="index">
  <tr>
    <th style="width: 90px; text-align: center;"></th>
    <th>所属名</th>
  </tr>
  <% @items.each do |item| -%>
    <tr cycle="<%= cycle '', 'cycle' %>">
      <td><%= link_to '展開', {action: :temp_index, g_cat: item.parent_g_order} %></td>
      <td><%= item.parent_g_name %></td>
    </tr>
  <% end -%>
</table>
