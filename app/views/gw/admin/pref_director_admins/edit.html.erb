<div class="piece AdminMessage">
<div class="pieceHeader">
<h2 class="pieceTitle">部課長在庁表示管理</h2>
</div><!--pieceHeader-->

<div class="pieceBody">
<%= render 'header_new_botton' %>
<%= render 'tree_str' %>

<%= form_for @item, :as => :item, :url => {action: :update}, :html => {method: :put, id: 'itemForm'} do |f| %>
  <%= f.error_messages %>
  <%= render 'form', f: f %>
  <div class="submitters"><%= f.submit '保存' %></div>
<% end %>

</div><!--pieceBody-->
</div><!--piece-->
