<div class="piece memo form">
<%= render '/gw/admin/memos/piece_header' %>
<div class="pieceBody">
<div class="pieceBodyHeader">
<%= render 'tabmenu' %>
</div>

<div class="pieceBodyBody">
<%= form_for :item, :url => {action: :create}, :html => {:multipart => true, :id => 'itemForm'} do |f| %>
  <%= f.error_messages %>
  <%= render 'form', f: f %>
  <%= quote_attrs(params) %>
  <div class="submitters"><%= f.submit "送信する" %></div>
<% end %>
</div>
</div>
</div>
