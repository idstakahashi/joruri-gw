<div class="piece prop form">
<div class="pieceHeader">
<%= render '/gw/admin/prop_others/piece_header' -%>
</div>
<div class="pieceBody">
<div class="pieceBodyHeader">
  <%= render '/gw/admin/prop_others/prop_master_menu' %>
  <%= render 'titleline' %>
</div>
<div class="pieceBodyBody">

<%= form_for :item, :url => {:action => :create}, :html => {:id => 'itemForm'} do |f| %>
  <%= f.error_messages -%>
  <%= render 'form', f: f -%>
  <div class="preserve">
    <%= f.submit '保存' %>
  </div>
<% end -%>

</div>
</div>
</div>
