<div class="piece prop form">
<div class="pieceHeader">
<%= render 'piece_header' -%>
</div>
<div class="pieceBody">
<div class="pieceBodyHeader">
  <%= render 'prop_master_menu' %>
  <%= render 'titleline' %>
</div>
<div class="pieceBodyBody">

<%= form_for :item, :url => {action: :update}, :html => {method: :put, multipart: true, id: 'itemForm'} do |f| -%>
  <%= f.error_messages -%>
  <%= render 'form', f: f -%>
  <%= raw(submit_for_update f, :js_submitter => 'my_submit()') -%>
<% end -%>

</div>
</div>
</div>
