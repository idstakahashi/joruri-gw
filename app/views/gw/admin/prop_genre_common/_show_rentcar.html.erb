<table class="show">
  <tr>
    <th>予約</th>
    <td><%= @item.reserved_state_show %></td>
  </tr>
  <tr>
    <th>表示順</th>
    <td><%= @item.sort_no %></td>
  </tr>
  <tr>
    <th>車名 </th>
    <td><%= @item.car_model%></td>
  </tr>
  <tr>
    <th>号車 </th>
    <td><%= @item.name %></td>
  </tr>
  <tr>
    <th>車種 </th>
    <td><%= @item.type_id_show %></td>
  </tr>
  <tr>
    <th>登録番号 </th>
    <td><%= @item.register_no%></td>
  </tr>
  <tr>
    <th>排気量 </th>
    <td><%= @item.exhaust %></td>
  </tr>
  <tr>
    <th>年式 </th>
    <td><%= @item.year_type %></td>
  </tr>
  <tr>
    <th>走行距離</th>
    <td><%= @item.travelled_km %></td>
  </tr>
  <tr>
    <th>備考</th>
    <td><%= @item.comment %></td>
  </tr>
  <% if @is_gw_admin %>
  <tr>
    <th>削除</th>
    <td><%= @item.delete_state_show %></td>
  </tr>
  <% end %>
</table>
