<% if @items.blank? %>
  <%= show_notice %>
<% else %>
  <table class="index">
    <tr>
      <th class="action"></th>
      <th>予約</th>
      <th>表示順</th>
      <th>車名</th>
      <th>号車</th>
      <th>車種</th>
      <th>登録番号</th>
      <th>年式</th>
      <th>管理所属</th>
      <% if @is_admin %>
      <th>削除</th>
      <% end %>
    </tr>
    <% @items.each do |item| %>
    <tr class="<%= cycle '', 'cycle' %>">
      <td><%= link_to "詳細", {:action => :show, :id => item.id} %></td>
      <td><%= item.reserved_state_show %></td>
      <td><%= item.sort_no %></td>
      <td><%= item.car_model %></td>
      <td><%= item.name %></td>
      <td><%= item.type_id_show %></td>
      <td><%= item.register_no %></td>
      <td><%= item.year_type %></td>
      <td><%= item.gname %></td>
      <% if @is_admin %>
      <td><%= item.delete_state_show %></td>
      <% end %>
    </tr>
    <% end %>
  </table>
  <%= paginate @items %>
<% end %>
