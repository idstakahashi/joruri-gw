<div class="bbsDocList circular">
<div class="pieceHeader"><h2 class="pieceTitle"><%= Page.title %></h2></div>

<div class="pieceBody">
<%= render 'header' %>

<%= form_for :item, :url => {:action => :create}, :html => {:method => :post, :multipart => true, :id => 'itemForm'} do |f| %>
  <%= f.error_messages %>
  <%= render 'form', f: f %>
  <%= submit_for f ,:caption => '保存する' %>
<% end %>

</div>
</div>
