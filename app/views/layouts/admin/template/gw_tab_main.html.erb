<!DOCTYPE html>
<html lang="ja">
<head>
<%= render 'layouts/admin/gw/header_tag' %>
<link rel="stylesheet" type="text/css" href="/layout/gw-tab-main/style.css" />
</head>
<body <%= Page.body_id %>>
<div id="container">

<!--ここからヘッダー-->
<div id="header">
<div id="headerBody">
<%= render 'layouts/admin/gw/header' %>
</div>
</div>
<!--ここまでヘッダー-->

<!--ここからバナー-->
<div id="banner">
<div id="bannerBody">
<%= render_component :controller => 'gw/admin/piece/header_menus', :action => 'index', :piece_param => '61' %>
</div>
</div>
<!--ここまでバナー-->

<!--ここからバナー-->
<div id="navi">
<div id="naviBody">
<%= render 'layouts/admin/gw/notice' %>
<%= render_component :controller => 'gw/admin/piece/pref_soumu_messages', :action => 'index', :piece_param => params[:id] %>
<%= render_component :controller => 'gw/admin/piece/tabs', :action => 'index', :piece_param => params[:id] %>
</div>
</div>
<!--ここまでバナー-->

<!--ここからメインコンテンツ-->
<div id="main">
<div id="wrapper">
<!--ここからコンテンツ-->
<div id="content">
<div id="contentContainer">
<div id="contentHeader"></div>
<div id="contentBody">
<div class="page">
<div class="body">
<%= yield %>
</div></div>
</div>
<div id="contentFooter"></div>
</div>
</div>
<!--ここまでコンテンツ-->
<!--ここから左サイド-->
<div id="menu">
<div id="menuContainer">
<div id="menuHeader"></div>
<div id="menuBody"></div>
<div id="menuFooter"></div>
</div>
</div>
<!--ここまで左サイド-->
</div>
<!--ここから右サイド-->
<div id="links">
<div id="linksContainer">
<div id="linksHeader"></div>
<div id="linksBody">右サイド</div>
<div id="linksFooter"></div>
</div>
</div>
<!--ここまで右サイド-->
</div>
<!--ここまでメインコンテンツ-->

<!--ここからフッター-->
<div id="footer">
<div id="footerContainer">
<div id="footerHeader"></div>
<div id="footerBody">
<%= render 'layouts/admin/gw/footer' %>
</div>
<div id="footerFooter"></div>
</div>
</div>
<!--ここまでフッター-->

</div>
</body>
</html>